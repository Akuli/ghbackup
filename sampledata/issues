    "title": "Some way to do inheritance",
    "user": {
      "login": "Akuli",
      "id": 18505570,
      "node_id": "MDQ6VXNlcjE4NTA1NTcw",
      "avatar_url": "https://avatars.githubusercontent.com/u/18505570?v=4",
